{
  "name": "template-vite-react",
  "description": "A template for react projects built with vite",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": "Mike Valeriano <mkvlrn@proton.me>",
  "repository": {
    "type": "git",
    "url": "git@github.com:mkvlrn/template-vite-react"
  },
  "keywords": ["vite", "react", "esm", "template"],
  "scripts": {
    "build": "NODE_ENV=production npx vite build",
    "dev": "vite",
    "start": "vite preview",
    "test": "vitest run",
    "test:integration": "vitest",
    "test:e2e": "vitest",
    "tidy": "biome check --fix --no-errors-on-unmatched",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.6.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
